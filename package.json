{
  "name": "comore",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "setup": "corepack enable pnpm",
    "build": "react-router build",
    "preview": "pnpm build && vite preview",
    "deploy": "pnpm build && wrangler deploy",
    "dev": "react-router dev",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "drizzle-kit migrate",
    "db:studio": "LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "start": "react-router-serve ./build/server/index.js",
    "cf-typegen": "wrangler types",
    "typecheck": "react-router typegen && tsc",
    "lint": "npx oxlint && npx eslint && knip",
    "fmt": "prettier -w . --experimental-cli",
    "postinstall": "npm run cf-typegen"
  },
  "dependencies": {
    "@react-router/node": "7.5.3",
    "@react-router/serve": "7.5.3",
    "isbot": "5.1.27",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.5.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.9.4",
    "@libsql/client": "0.15.9",
    "@react-router/dev": "7.5.3",
    "@secretlint/secretlint-rule-preset-recommend": "10.2.0",
    "@tailwindcss/vite": "4.1.4",
    "@types/node": "20",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "dotenv": "17.2.0",
    "drizzle-kit": "0.31.4",
    "drizzle-orm": "0.44.2",
    "eslint-plugin-oxlint": "1.6.0",
    "knip": "5.61.3",
    "lefthook": "1.12.2",
    "prettier": "3.6.2",
    "secretlint": "10.2.0",
    "tailwindcss": "4.1.4",
    "typescript": "5.8.3",
    "vite": "6.3.3",
    "vite-tsconfig-paths": "5.1.4",
    "wrangler": "4.24.3"
  }
}
